<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="py.gov.contrataciones.reikuaa.view.AdjudicacionesActivity">

    <include
        android:id="@+id/toolbar"
        layout="@layout/toolbar"
        />

    <include
        layout="@layout/progress_item"
        android:id="@+id/AdjProgress"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/toolbar"
        android:visibility="gone"
        />

    <android.support.v7.widget.RecyclerView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/toolbar"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="32dp"
        android:id="@+id/cuerpoAdjudicacion"
        android:scrollbars="vertical"
        android:layout_marginTop="15dp" />

    <include
        layout="@layout/error404adjudicacion"
        android:id="@+id/error404Adjudicacion"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/toolbar"
        android:layout_marginLeft="40dp"
        android:layout_marginRight="40dp"
        android:visibility="gone"
        />

    

</RelativeLayout>
